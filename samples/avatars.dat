# animation registry version 0.3
version 3                               # keep this around

# Movement
#walk=shanub01
#run=
#jump=

# Gravity
# Plays when falling for ~1s
#fall=qfall1s

# Fly
#fly=qfly2
# Idling while flying
#hover=qhover1

# Swim
#swim=qswim
# Idling while underwater
#float=qfloat

# Idle
# Just stopped moving.
#idle=hotep02
# Plays when Idle is over
#wait=hotep02
# Plays when Wait is over, then loops back to wait.
#endwait=shwaiten

# Misc
# Bug: 3.6 (b568) doesn't play this one
#warp=

